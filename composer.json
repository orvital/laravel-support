{
    "name": "orvital/laravel-support",
    "description": "Laravel support package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jona Goldman",
            "email": "jonagoldman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/session": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Orvital\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orvital\\Support\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orvital\\Support\\SupportServiceProvider"
            ]
        }
    }
}